{
  "name": "@alorel/rollup-plugin-fast-tsc",
  "version": "0.0.0",
  "description": "A faster typescript plugin for rollup",
  "main": "index.js",
  "repository": "https://github.com/Alorel/rollup-plugin-fast-tsc.git",
  "types": "index.d.ts",
  "typings": "index.d.ts",
  "author": "Arturas Molcanovas <a.molcanovas@gmail.com>",
  "license": "MIT",
  "scripts": {
    "rollup": "del-cli dist && tsc",
