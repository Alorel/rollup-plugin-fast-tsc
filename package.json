    "lint": "tslint -s node_modules/custom-tslint-formatters/formatters -t grouped -p .",
    "lint:fix": "yarn run lint --fix",
    "rollup:watch": "yarn run rollup --sourceMap --watch"
  },
  "peerDependencies": {
    "rollup": "^2.6.0 || ^1.30.0",
    "typescript": "^3.7.0"
  },
  "devDependencies": {
    "@alorel-personal/conventional-changelog-alorel": "^2.1.3",
    "@alorel-personal/tslint-rules": "^5.0.7",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/exec": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.0.5",
    "@semantic-release/npm": "^7.0.5",
    "@types/cross-spawn": "^6.0.1",
    "@types/node": "^13.13.4",
    "@types/tmp": "^0.1.0",
    "custom-tslint-formatters": "^2.4.0",
    "del-cli": "^3.0.0",
    "rollup": "^2.7.3",
    "semantic-release": "^17.0.7",
    "tslib": "^1.11.1",
    "tslint": "^6.1.2",
    "typescript": "^3.8.3"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com/alorel"
  },
  "dependencies": {
    "cross-spawn": "^7.0.2",
    "tmp": "^0.2.0"
  }
